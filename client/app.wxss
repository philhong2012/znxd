/**app.wxss**/
@import 'weui.wxss';
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 


/* pages/login/index.wxss */
.input-text {
  border: darkgray 1px solid;
  padding: 2px;
  line-height: 50px;
}

.section {
  margin: 5px;
}

.picker {
  border: darkgray 1px solid;
}

.mask{
  width:100%;
  height:100%;
  position:fixed;
  background-color:#999;
  z-index:9999;
  top:0;
  left:0;
  opacity:0.5;
}



